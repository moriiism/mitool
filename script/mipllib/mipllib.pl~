#!/usr/bin/perl

# For example, you can load this script by
# $mipllib_dir = "/opt/mitool/pro/script/mipllib";
# require("$mipllib_dir/mipllib.pl");

use strict;
use warnings;
use Cwd;
use File::Basename;
use File::Temp;
use File::Spec;
use POSIX;
use Getopt::Long;
no warnings qw(once);

$ENV{'LANG'} = "C";

package mipllib;

my $varlib    = sprintf("%s/script/mipllib/varlib.pl", $ENV{'MITOOL'});
my $templib   = sprintf("%s/script/mipllib/templib.pl", $ENV{'MITOOL'});
my $locklib   = sprintf("%s/script/mipllib/locklib.pl", $ENV{'MITOOL'});
my $utillib   = sprintf("%s/script/mipllib/utillib.pl", $ENV{'MITOOL'});
my $timelib   = sprintf("%s/script/mipllib/timelib.pl", $ENV{'MITOOL'});
my $mathlib   = sprintf("%s/script/mipllib/mathlib.pl", $ENV{'MITOOL'});
my $headaslib = sprintf("%s/script/mipllib/headaslib.pl", $ENV{'MITOOL'});
my $maxilib   = sprintf("%s/script/mipllib/maxilib.pl", $ENV{'MITOOL'});
my $coordlib  = sprintf("%s/script/mipllib/coordlib.pl", $ENV{'MITOOL'});
my $htmllib   = sprintf("%s/script/mipllib/htmllib.pl", $ENV{'MITOOL'});
my $instrlib  = sprintf("%s/script/mipllib/instrlib.pl", $ENV{'MITOOL'});
my $physlib   = sprintf("%s/script/mipllib/physlib.pl", $ENV{'MITOOL'});

require($varlib);
require($templib);
require($locklib);
require($utillib);
require($timelib);
require($mathlib);
require($headaslib);
require($maxilib);
require($coordlib);
require($htmllib);
require($instrlib);
require($physlib);


1;
